	<Script file="constants.lua"/>
	<Script file="localization.lua"/>
	<Script file="localization.de.lua"/>
	<Script file="localization.fr.lua"/>
	<Script file="localization.kr.lua"/>
	<Script file="SarfSmartPet.lua"/>

	<Frame name="SarfSmartPetFrame" toplevel="true" enableMouse="true" hidden="true">
		<Scripts> 
			<OnLoad> 
				SarfSmartPet_OnLoad();
			</OnLoad>
			<OnEvent>
				SarfSmartPet_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
	<GameTooltip name="SarfSmartPetTooltip" frameStrata="TOOLTIP" hidden="true" inherits="GameTooltipTemplate"/>
</Ui>