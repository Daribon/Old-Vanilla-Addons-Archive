				</Frames>
            </Frame>
			 <Frame name="$parentSearchBox2" inherits="AceGUIOptionsBoxTemplate">
                <Frames>
				</Frames>
            </Frame>
        </Frames>
    </Frame>

	<Script file="KC_ItemsLinkviewLocals.lua"/>
	<Script file="KC_ItemsLinkviewGUI.lua"/>
	<Script file="KC_ItemsLinkviewAdvSearch.lua"/>
